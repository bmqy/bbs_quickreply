{
   "author": "bmqy",
   "browser_action": {
      "default_icon": "images/icon_128.png",
      "default_title": "论坛快捷回帖"
   },
   "content_scripts": [ {
      "js": ["js/bbs_quickreply.user.js" ],
      "matches": ["*://*/thread*.*", "*://*/forum.php?mod=viewthread*", "*://*/forum.php?mod=post*"]
   } ],
   "description": "使用自定义内容或本扩展预定义的回帖内容，快捷回复支持的论坛的发帖！",
   "icons": {
      "128": "images/icon_128.png",
      "16": "images/icon_16.png",
      "48": "images/icon_48.png",
      "64": "images/icon_64.png"
   },
   "manifest_version": 2,
   "name": "论坛快捷回帖",
   "short_name": "快捷回帖",
   "permissions": [ "storage" ],
   "key": "",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1"
}
